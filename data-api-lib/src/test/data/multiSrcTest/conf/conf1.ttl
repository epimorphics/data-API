@prefix eg:  <http://www.epimorphics.com/test/dsapi/sprint3/multiSrc/> .
@prefix data:  <http://www.epimorphics.com/test/dsapi/sprint3/multiSrc/data1> .

eg:dataset1 a dsapi:Dataset;
    rdfs:label "dataset1";
    dct:description "Multi-source dataset1";
    dsapi:baseQuery "{?item rdf:type eg:Obs.}";
    dsapi:aspect 
        [ dsapi:property eg:label],
        [ dsapi:property eg:resource],
        [ dsapi:property eg:value];
    .
